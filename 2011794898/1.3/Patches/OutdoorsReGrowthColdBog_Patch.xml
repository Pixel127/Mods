            </li>
			<li Class="PatchOperationReplace">
                <xpath>/Defs/ThingDef[defName="RG-CB_Volopus"]/statBases/Beauty/text()</xpath>
                <value>3</value>
            </li>
			<li Class="PatchOperationReplace">
                <xpath>/Defs/ThingDef[defName="RG-CB_Dervish"]/statBases/Beauty/text()</xpath>
                <value>8</value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>/Defs/ThingDef[defName="RG-CB_SwampPod"]/statBases/Beauty/text()</xpath>
                <value>-5</value>
            </li>			
			
        </operations>
    </match>
    </Operation>
</Patch>