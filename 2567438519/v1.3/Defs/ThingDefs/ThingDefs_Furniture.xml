        <!--Counter-->
        <ThingDef ParentName="BuildingBase">
            <defName>LTS_Counter</defName>
            <label>counter</label>
            <description>A counter for storing kitchen utensils. Can work as a table as well.</description>
            <thingClass>Building</thingClass>
            <category>Building</category>
            <graphicData>
                <texPath>Furniture/Counter_Atlas</texPath>
                <graphicClass>Graphic_Single</graphicClass>
                <linkType>CornerFiller</linkType>
                <linkFlags>
                    <li>Custom1</li>
                </linkFlags>
                <shadowData>
                    <volume>(0.3, 0.5, 0.3)</volume>
                    <offset>(0,0,-0.23)</offset>
                </shadowData>
                <damageData>
                    <rect>(0.35,0.1,0.3,0.7)</rect>
                </damageData>
            </graphicData>
            <uiIconPath>Furniture/Counter_UI</uiIconPath>
            <uiIconScale>1</uiIconScale>
            <costStuffCount>20</costStuffCount>
            <statBases>
                <MaxHitPoints>50</MaxHitPoints>
                <WorkToBuild>150</WorkToBuild>
                <Flammability>1.0</Flammability>
                <Beauty>2</Beauty>
            </statBases>
            <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
            <altitudeLayer>Building</altitudeLayer>
            <passability>PassThroughOnly</passability>
            <pathCost>80</pathCost>
            <blockWind>false</blockWind>
            <fillPercent>0.2</fillPercent>
            <placingDraggableDimensions>1</placingDraggableDimensions>
            <tickerType>Never</tickerType>
            <rotatable>false</rotatable>
            <selectable>true</selectable>
            <neverMultiSelect>true</neverMultiSelect>
            <useStuffTerrainAffordance>true</useStuffTerrainAffordance>
            <terrainAffordanceNeeded>GrowSoil</terrainAffordanceNeeded>
            <designationCategory>Furniture</designationCategory>
            <canOverlapZones>false</canOverlapZones>
            <stuffCategories>
                <li>Woody</li>
            </stuffCategories>
            <building>
                <isInert>true</isInert>
                <isFence>true</isFence>
                <isPlaceOverableWall>true</isPlaceOverableWall>
                <ai_chillDestination>false</ai_chillDestination>
                <ai_neverTrashThis>true</ai_neverTrashThis>
                <blueprintGraphicData>
                    <texPath>Furniture/CounterBlueprint_Atlas</texPath>
                    <linkType>CornerFiller</linkType>
                    <linkFlags>
                        <li>Custom1</li>
                    </linkFlags>
                </blueprintGraphicData>
            </building>
            <researchPrerequisites>
                <li>ComplexFurniture</li>
            </researchPrerequisites>
            <surfaceType>Eat</surfaceType>
        </ThingDef>
        <!--Hanging prison bed-->
        <ThingDef ParentName="BedWithQualityBase">
            <defName>LTS_HangingBed</defName>
            <label>hanging bed</label>
            <description>A hanging bed. Not as cozy as a regular bed, but better than the floor. Oft used for that prison feel to show people they are not worthy of a real bed. Some like it, though.</description>
            <graphicData>
                <texPath>Furniture/HangingPrisonBed</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <shaderType>CutoutComplex</shaderType>
                <drawSize>(1,2)</drawSize>
            </graphicData>
            <comps>
                <li Class="CompProperties_AffectedByFacilities">
                    <linkableFacilities>
                        <li>EndTable</li>
                        <li>Dresser</li>
                        <li MayRequire="Ludeon.RimWorld.Ideology">SleepAccelerator</li>
                    </linkableFacilities>
                </li>
            </comps>
            <statBases>
                <MaxHitPoints>100</MaxHitPoints>
                <Flammability>1.0</Flammability>
                <Beauty>0</Beauty>
                <WorkToBuild>500</WorkToBuild>
                <Mass>20</Mass>
                <BedRestEffectiveness>0.90</BedRestEffectiveness>
                <Comfort>0.70</Comfort>
                <ImmunityGainSpeedFactor>1.05</ImmunityGainSpeedFactor>
            </statBases>
            <size>(1,2)</size>
            <stuffCategories>
                <li>Metallic</li>
                <li>Woody</li>
                <li>Stony</li>
            </stuffCategories>
            <costStuffCount>30</costStuffCount>
            <building>
                <bed_showSleeperBody>False</bed_showSleeperBody>
                <bed_healPerDay>4</bed_healPerDay>
            </building>
            <fillPercent>0.4</fillPercent>
            <pathCost>42</pathCost>
            <staticSunShadowHeight>0.17</staticSunShadowHeight>
            <castEdgeShadows>True</castEdgeShadows>
            <researchPrerequisites>
                <li>ComplexFurniture</li>
            </researchPrerequisites>
            <uiIconScale>0.90</uiIconScale>
        </ThingDef>
    </Defs>
