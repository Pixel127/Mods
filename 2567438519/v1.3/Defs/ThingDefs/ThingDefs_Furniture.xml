        <!--Tables-->
        <!--Norraryd-->
        <ThingDef ParentName="TableGatherSpotBase">
            <defName>LTS_Norraryd</defName>
            <label>Norraryd</label>
            <description>Ever wanted a sturdy table able to withstand the test of time? This is not that table, but it's a table with a strange scandinavian name, and that is much better.</description>
            <graphicData>
                <texPath>Furniture/Norraryd</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <damageData>
                    <cornerTL>Damage/Corner</cornerTL>
                    <cornerTR>Damage/Corner</cornerTR>
                    <cornerBL>Damage/Corner</cornerBL>
                    <cornerBR>Damage/Corner</cornerBR>
                </damageData>
                <drawSize>(2.9,3.3)</drawSize>
                <drawOffset>(0,0,-0.15)</drawOffset>
            </graphicData>
            <uiIconScale>0.8</uiIconScale>
            <statBases>
                <MaxHitPoints>125</MaxHitPoints>
                <WorkToBuild>1250</WorkToBuild>
                <Mass>14</Mass>
                <Flammability>1.0</Flammability>
                <Beauty>2.5</Beauty>
                <StyleDominance MayRequire="Ludeon.RimWorld.Ideology">25</StyleDominance>
            </statBases>
            <stuffCategories>
                <li>Woody</li>
            </stuffCategories>
            <researchPrerequisites>
                <li>ComplexFurniture</li>
            </researchPrerequisites>
            <size>(2,3)</size>
            <costStuffCount>125</costStuffCount>
        </ThingDef>
