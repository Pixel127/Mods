        <ThingDef ParentName="ArtableFurnitureBase">
            <defName>LTS_Klimpfjall</defName>
            <label>klimpfj채ll</label>
            <description>A comfortable and attractive chair with a strange scandinavian name. Can be used at tables, work stations, and elsewhere.</description>
            <graphicData>
                <texPath>Furniture/Klimpfj채ll</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <damageData>
                    <rectN>(0.25,0.234375,0.5,0.28125)</rectN>
                    <rectS>(0.25,0.234375,0.5,0.28125)</rectS>
                    <rectE>(0.28125,0.25,0.453125,0.25)</rectE>
                    <rectW>(0.28125,0.25,0.453125,0.25)</rectW>
                </damageData>
                <shadowData>
                    <volume>(0.23, 0.26, 0.23)</volume>
                </shadowData>
