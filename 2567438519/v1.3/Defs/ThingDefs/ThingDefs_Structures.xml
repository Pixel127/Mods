        <!-- Archs -->
        <ThingDef ParentName="BuildingBase">
            <defName>LTS_ArchStone</defName>
            <label>stone arch</label>
            <description>An arch made out of stone.</description>
            <thingClass>Building</thingClass>
            <category>Building</category>
            <graphicData>
                <texPath>Buildings/ArchedWayStone</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <drawSize>(1.5,1.6)</drawSize>
                <drawOffsetNorth>(0,0,0.3)</drawOffsetNorth>
                <drawOffsetSouth>(0,0,0.3)</drawOffsetSouth>
                <drawOffsetEast>(0,0,0.22)</drawOffsetEast>
