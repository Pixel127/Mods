        <!-- Pillars -->
        <ThingDef ParentName="BuildingBase">
            <defName>LTS_Pillar</defName>
            <label>pillar</label>
            <description>A hard pillar capable of holding a roof. Impassable and an alternative to cover like shown in a certain movie.</description>
            <designationCategory>Structure</designationCategory>
            <altitudeLayer>BuildingOnTop</altitudeLayer>
            <passability>PassThroughOnly</passability>
            <fillPercent>0.5</fillPercent>
            <statBases>
                <MaxHitPoints>25</MaxHitPoints>
                <WorkToBuild>750</WorkToBuild>
                <Mass>20</Mass>
                <Flammability>1.0</Flammability>
                <Beauty>2</Beauty>
                <StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
            </statBases>
            <stuffCategories>
                <li>Metallic</li>
                <li>Woody</li>
                <li>Stony</li>
            </stuffCategories>
            <costStuffCount>20</costStuffCount>
            <holdsRoof>true</holdsRoof>
            <canOverlapZones>false</canOverlapZones>
            <rotatable>false</rotatable>
            <fertility>0</fertility>
            <specialDisplayRadius>6.9</specialDisplayRadius>
            <!-- must be kept in sync with roof hold-up radius -->
            <building>
                <isInert>true</isInert>
                <ai_chillDestination>false</ai_chillDestination>
            </building>
            <graphicData>
                <drawSize>(1.5,1.5)</drawSize>
                <drawOffset>(0,0,0.2)</drawOffset>
                <texPath>Buildings/SimplePillar</texPath>
                <graphicClass>Graphic_Single</graphicClass>
                <shadowData>
                    <volume>(0.3, 0.5, 0.3)</volume>
                    <offset>(0,0,-0.23)</offset>
                </shadowData>
                <damageData>
                    <rect>(0.2,0.2,0.6,0.6)</rect>
                </damageData>
            </graphicData>
            <comps>
                <li Class="CompProperties_Styleable" />
            </comps>
        </ThingDef>


