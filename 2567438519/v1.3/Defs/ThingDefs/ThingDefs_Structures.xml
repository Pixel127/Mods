<?xml version="1.0" encoding="utf-8" ?>
    <Defs>
        <ThingDef Abstract="True" ParentName="DoorBase" Name="LTS_DoorBase">
            <staticSunShadowHeight>0</staticSunShadowHeight>
            <graphicData>
                <graphicClass>Graphic_Multi</graphicClass>
                <shadowData>
                    <volume>(0.3, 0.5, 0.3)</volume>
                </shadowData>
                <damageData>
                    <enabled>false</enabled>
                </damageData>
            </graphicData>
        </ThingDef>

        <!-- Hedge -->
        <ThingDef ParentName="BuildingBase">
            <defName>LTS_Hedge</defName>
            <label>hedge</label>
            <description>A hedge that can be used as a fence! Fences only block certain kinds of animals.</description>
            <thingClass>Building</thingClass>
            <category>Building</category>
            <graphicData>
                <texPath>Buildings/Hedge_Atlas</texPath>
                <graphicClass>Graphic_Single</graphicClass>
                <linkType>Asymmetric</linkType>
                <linkFlags>
                    <li>Fences</li>
                </linkFlags>
                <asymmetricLink>
                    <linkFlags>
                        <li>Wall</li>
                        <li>Rock</li>
                    </linkFlags>
                </asymmetricLink>
                <shadowData>
                    <volume>(0.3, 0.5, 0.3)</volume>
                    <offset>(0,0,-0.23)</offset>
                </shadowData>
                <damageData>
                    <rect>(0.35,0.1,0.3,0.7)</rect>
                </damageData>
            </graphicData>
            <uiIconPath>Buildings/Hedge_UI</uiIconPath>
            <uiIconScale>1</uiIconScale>
            <uiIconColor>(255,255,255)</uiIconColor>
            <costStuffCount>5</costStuffCount>
            <statBases>
                <MaxHitPoints>50</MaxHitPoints>
                <WorkToBuild>150</WorkToBuild>
                <Flammability>1.0</Flammability>
                <Beauty>2</Beauty>
            </statBases>
            <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
            <altitudeLayer>Building</altitudeLayer>
            <passability>PassThroughOnly</passability>
            <pathCost>80</pathCost>
            <blockWind>false</blockWind>
            <fillPercent>0.2</fillPercent>
            <placingDraggableDimensions>1</placingDraggableDimensions>
            <tickerType>Never</tickerType>
            <rotatable>false</rotatable>
            <selectable>true</selectable>
            <neverMultiSelect>true</neverMultiSelect>
            <useStuffTerrainAffordance>true</useStuffTerrainAffordance>
            <terrainAffordanceNeeded>GrowSoil</terrainAffordanceNeeded>
            <designationCategory>Structure</designationCategory>
            <canOverlapZones>false</canOverlapZones>
            <fertility>0</fertility>
            <stuffCategories>
                <li>Woody</li>
            </stuffCategories>
            <colorPerStuff>
                <li>
                    <stuff>WoodLog</stuff>
                    <color>(255,255,255)</color>
                </li>
            </colorPerStuff>
            <building>
                <isInert>true</isInert>
                <isFence>true</isFence>
                <isPlaceOverableWall>true</isPlaceOverableWall>
                <ai_chillDestination>false</ai_chillDestination>
                <ai_neverTrashThis>true</ai_neverTrashThis>
                <relatedBuildCommands>
                    <li>PenMarker</li>
                    <li>FenceGate</li>
                </relatedBuildCommands>
                <blueprintGraphicData>
                    <texPath>Buildings/HedgeBlueprint_Atlas</texPath>
                    <linkType>Asymmetric</linkType>
                    <linkFlags>
                        <li>Fences</li>
                    </linkFlags>
                    <asymmetricLink>
                        <linkFlags>
                            <li>Wall</li>
                            <li>Rock</li>
                        </linkFlags>
                    </asymmetricLink>
                </blueprintGraphicData>
            </building>
            <researchPrerequisites>
                <li>ComplexFurniture</li>
            </researchPrerequisites>
        </ThingDef>
