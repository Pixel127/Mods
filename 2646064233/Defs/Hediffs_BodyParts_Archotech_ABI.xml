<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Archotech stomach -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>ArchotechVanometricStomach</defName>
    <label>vanometric stomach</label>
    <labelNoun>a vanometric stomach</labelNoun>
    <description>An installed vanometric stomach.</description>
    <descriptionHyperlinks><ThingDef>ArchotechVanometricStomach</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>ArchotechVanometricStomach</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.5</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <stages>
      <li>
        <hungerRateFactor>0.25</hungerRateFactor>
        <foodPoisoningChanceFactor>0</foodPoisoningChanceFactor>
      </li>
    </stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartArchotechBase">
    <defName>ArchotechVanometricStomach</defName>
    <label>vanometric stomach</label>
    <description>A micro-Archotech reactor combined with complex chemical synthesis replaces the body's need for food energy. Food is required only to replace matter lost from the body, reducing the user's intake needs significantly. This artificial stomach cannot get food poisoning. This incredible archotech device poses no health risk to the user.</description>
    <descriptionHyperlinks><RecipeDef>InstallArchotechVanometricStomach</RecipeDef></descriptionHyperlinks>
    <statBases>
      <MarketValue>2800</MarketValue>
      <Mass>2.5</Mass>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>InstallArchotechVanometricStomach</defName>
    <label>install vanometric stomach</label>
    <description>Install a vanometric stomach.</description>
    <descriptionHyperlinks>
      <ThingDef>ArchotechVanometricStomach</ThingDef>
      <HediffDef>ArchotechVanometricStomach</HediffDef>
    </descriptionHyperlinks>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Installing vanometric stomach.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ArchotechVanometricStomach</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ArchotechVanometricStomach</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Stomach</li>
    </appliedOnFixedBodyParts>
    <addsHediff>ArchotechVanometricStomach</addsHediff>
  </RecipeDef>

