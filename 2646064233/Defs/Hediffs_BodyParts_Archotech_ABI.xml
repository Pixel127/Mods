    <descriptionHyperlinks><RecipeDef>InstallArchotechCerebralTurbocharger</RecipeDef></descriptionHyperlinks>
    <statBases>
      <MarketValue>3200</MarketValue>
      <Mass>0.1</Mass>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
